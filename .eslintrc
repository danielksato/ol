{
    "extends": ["standard-react", "prettier", "plugin:prettier/recommended"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "jsx": true
    },
    "rules": {
        "no-console": "warn",
        "no-debugger": "warn",
        "no-underscore-dangle": "off",
        "no-useless-return": "off",
        "no-path-concat": "off",
        "prefer-const": "warn",
        "react/jsx-indent": "off",
        "react/jsx-indent-props": "off",
        "react/prop-types": "off",
        "node/no-deprecated-api": "off"
    },
    "globals": {
        "fetch": false,
        "it": false
    }
}
